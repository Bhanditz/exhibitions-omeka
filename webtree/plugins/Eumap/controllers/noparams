<div class="row" style="text-align:center;">
	<h1>TEST MAP</h1>
</div>


<div class="row">

	<div id="map_container" class="twelve columns inner">
		<div id="map">
		</div>
	</div>

	
	<script type="text/javascript">
		var mapLatitude	= 41.8900;
var mapLongitude	= 12.4918;
var mapOverlays = [];
var markers = [];
markers[markers.length] = {"lat": "41.8854", "lon": "12.4706", "pageId":"349"}; 
		
	
		jQuery(document).ready(function(){
				var map			= new L.Map('map');
				var osmUrl		= 'http://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png';
				var osmAttrib	= 'Map data &copy; <a href="http://openstreetmap.org">OpenStreetMap</a> contributors';
				
				var osm = new L.TileLayer(osmUrl,
						{
							minZoom: 12,
							maxZoom: 18,
							attribution: osmAttrib
						}
				);
				
				map.setView(
						new L.LatLng( mapLatitude, mapLongitude ),
						13);
				map.addLayer(osm);	

									var popup = L.popup();
					function onMapClick(e) {
					    popup
					        .setLatLng(e.latlng)
					        .setContent("You clicked the map at " + e.latlng.toString())
					        .openOn(map);
					}
					map.on('click', onMapClick);
								
				jQuery.each(mapOverlays, function(i, ob){
					var imgOverlay = L.imageOverlay(ob.uri, [[ob.nwLat, ob.nwLon], [ob.seLat, ob.seLon]] ).addTo(map);

					var sliderDiv = jQuery('<div class="slider">').appendTo('#map_container');
					
					sliderDiv.slider({
			            value: 100,
			            slide: function(e, ui) {
							imgOverlay.setOpacity(ui.value / 100);
			            }
			        });
			        
				
				});




				jQuery.each(markers, function(i, ob){

					var marker = L.marker(
							[
								parseFloat(ob.lat)
									,
								parseFloat(ob.lon)
							]
							)
					marker.addTo(map);
					

		    		marker.bindPopup('<div style="min-width:180px;height:0px;">');
		    		
					marker.on('click', function(e){

					jQuery.ajax({
						url:		"http://localhost/webtree/eumap/map/test?pageId=" + ob.pageId,
						dataType:	"json"
						}).done(function ( data ) {
							marker._popup.setContent('<a href="' + data.url + '"><h5>' + data.title + '</h5><img src="' + data.imgUrl + '"/></a>');
						});
					});
				});
		});
	
	</script>
	
	
	
</div>
	
